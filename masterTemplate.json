{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmRole": {
            "type": "string"
        },
        "virtualMachineAdminUserName": {
            "type": "string"
        },
        "numberofVMs": {
            "type": "int"
        },
        "virtualNetworkRG": {
            "type": "string"
        },
        "virtualNetworkName": {
            "type": "string"
        },
        "SubnetName": {
            "type": "string"
        },
        "PrivateIP": {
            "type": "array"
        },
        "virtualMachineSize": {
            "type": "string"
        },
        "VM-Admin-Password": {
            "type": "securestring"
        },
        "vmtags": {
            "type": "object"
        },
        "diagnosticStorageAccountName": {
            "type": "string"
        },
        "storagetags": {
            "type": "object"
        },
        "keyVaultName": {
            "type": "string"
        },
        "keyvaulttags": {
            "type": "object"
        },
        "objectId": {
            "type": "object"
        },
        "workspaceName": {
            "type": "string"
        },
        "sku": {
            "type": "string",
            "allowedValues": [
                "Free",
                "Standalone",
                "PerNode"
            ]
        },
        "baseUrl": {
            "type": "string"
        }
    },
    "variables": {
        "configuration": {
            "provisioningVMURL": "[concat(parameters('baseUrl'),'ge-ems/nestedTemplates/virtualmachine.json')]",
            "keyvaultURL": "[concat(parameters('baseUrl'),'ge-ems/nestedTemplates/keyvault.json')]",
            "resourcelocksURL": "[concat(parameters('baseUrl'),'ge-ems/nestedTemplates/resourcelocks.json')]",
            "diagStorageAccount":"[concat(parameters('baseUrl'),'ge-ems/nestedTemplates/diagStorageAccount.json')]"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "name": "ProvisioningVirtualMachine",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('configuration').provisioningVMURL]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "vmRole": {
                        "value": "[parameters('vmRole')]"
                    },
                    "objectId": {
                        "value": "[parameters('objectId')]"
                    },
                    "virtualMachineAdminUserName": {
                        "value": "[parameters('virtualMachineAdminUserName')]"
                    },
                    "numberofVMs": {
                        "value": "[parameters('numberofVMs')]"
                    },
                    "virtualNetworkRG": {
                        "value": "[parameters('virtualNetworkRG')]"
                    },
                    "virtualNetworkName": {
                        "value": "[parameters('virtualNetworkName')]"
                    },
                    "SubnetName": {
                        "value": "[parameters('SubnetName')]"
                    },
                    "PrivateIP": {
                        "value": "[parameters('PrivateIP')]"
                    },
                    "virtualMachineSize": {
                        "value": "[parameters('virtualMachineSize')]"
                    },
                    "VM-Admin-Password": {
                        "value": "[parameters('VM-Admin-Password')]"
                    },
                    "vmtags": {
                        "value": "[parameters('vmtags')]"
                    },
                    "baseUrl": {
                        "value": "[parameters('baseUrl')]"
                    }
                }
            }
        }
    ],
    "outputs": {}
}
